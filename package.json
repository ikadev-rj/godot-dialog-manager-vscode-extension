{
  "name": "godot-dialog-manager-node",
  "displayName": "godot-dialog-manager-node",
  "description": "to handle godot dialog manager files",
  "version": "0.0.1",
  "publisher": "ikadev-rj",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikadev-rj/godot-dialog-manager-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/ikadev-rj/godot-dialog-manager-vscode-extension/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "dialogue",
      "aliases": ["Godot Dialog Manager", "dialogue"],
      "extensions": [".dialogue"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dialogue",
      "scopeName": "source.dialogue",
      "path": "./syntaxes/dialogue.tmLanguage.json"
    }]
  }
}
